student_heights = input("Input a list of student heights ").split()
for n in range(0, len(student_heights)):
  student_heights[n] = int(student_heights[n])

#create variables for total height and number of students
total_height = 0
num_students = 0

#loop through the list and increment total height and num students for each student in the list
for student in student_heights:
    total_height += student
    num_students += 1

avg_height = total_height / num_students

rounded_final = int(round(avg_height))

print(rounded_final)
